<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Waffle order queue</title>

  <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Satisfy|Bree+Serif|Candal|PT+Sans">
  <link rel="stylesheet" type="text/css" href="/static/css/font-awesome.min.css">
  <link rel="stylesheet" type="text/css" href="/static/css/bootstrap.min.css">
  <link rel="stylesheet" type="text/css" href="/static/css/style.css">

  <style>
    #header {
      background-color: #FFB03B;
    }
    .menu-icon {
      margin-top:0px;
    }
    table {
      width: 100%;
      margin-top: 20px;
      margin-bottom: 20px;
    }
    tr {
      background-color: #f0f0f0;
      border-bottom: 1px dotted grey;
    }
    th {
      color: white;
      padding-left: 10px;
    }
    td {
      padding: 10px;
    }
    .heading {
      background-color: darkgrey;
      border:none;
    }
  </style>
</head>

<body>
  <!--banner-->


      <header id="header">
        <div class="container">
          <div id="mySidenav" class="sidenav">
            <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
            <a href="/">Home</a>
          </div>
          <!-- Use any element to open the sidenav -->
          <a href="/"><span class="pull-right menu-icon menu-icon-queue"><img src="/static/img/home-icon2.png" width="23px"> Home</span></a>
        </div>
      </header>
      <div class="container">
        <div class="col-md-5  text-center waitingcontainer">
          <div class="row">
            <div class="col-md-12 text-center marb-35" style="float: none;">
              <h1 class="header-h">Level 10 Waiting List</h1>
            </div>


            <% if (submission!='') { %>
              <% for (var i=0; i<submission.length; i++) { %>
                <% if (submission[i].level == 'l10') { %>
                  <div class="queuelist">
                    <p class="queue-email"><%= submission[i].email %></p>
                  </div>
                <% } %>
              <% } %>
            <% } else { %>
              <p style="text-align: center; margin-top: 2rem;">No one is in the queue!</p>
            <% } %>
          </div>
        </div>

        <div class="col-md-2 text-center">
        </div>

        <div class="col-md-5  text-center waitingcontainer">
          <div class="row">
            <div class="col-md-12 text-center marb-35" style="float: none;">
              <h1 class="header-h">Level 19 Waiting List</h1>
            </div>


            <% if (submission!='') { %>
              <% for (var i=0; i<submission.length; i++) { %>
                <% if (submission[i].level == 'l19') { %>
                  <div class="queuelist">
                    <p class="queue-email"><%= submission[i].email %></p>
                  </div>
                <% } %>
              <% } %>
            <% } else { %>
              <p style="text-align: center; margin-top: 2rem;">No one is in the queue!</p>
            <% } %>
          </div>
        </div>
      </div>



  <!-- / banner -->


  <script src="/static/js/jquery.min.js"></script>
  <script src="/static/js/jquery.easing.min.js"></script>
  <script src="/static/js/bootstrap.min.js"></script>
  <script src="/static/js/custom.js"></script>

</body>

</html>
